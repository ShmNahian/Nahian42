#include <stdio.h>

void check(int N) {
    int sum = 0;

    for (int i = 0; i < N; i++) {
        int a,b,c;
        scanf("%d %d", &a ,&b);
        c = a + b;
        sum+=c;
    }
    if (sum % 5 == 0) {
        printf("Answer is alright\n");
    } else {
        printf("Not matched\n");
    }
}

int main() {
    int N;
    scanf("%d", &N);
    check(N);

    return 0;
}
